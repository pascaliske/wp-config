{
  "name": "pi/wp-config",
  "description": "WordPress environment specific config setup.",
  "version": "2.0.1",
  "authors": [
    {
      "name": "Pascal Iske",
      "email": "info@pascal-iske.de",
      "homepage": "https://pascal-iske.de/"
    }
  ],
  "license": "MIT",
  "homepage": "https://pascal-iske.de/packages/",
  "keywords": [
    "wordpress", "stack", "composer", "vagrant", "wp"
  ],
  "support": {
    "issues": "https://github.com/pascaliske/wp-config/issues"
  },
  "autoload": {
    "psr-4": {
      "PI\\Configuration\\": "src/Configuration"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PI\\Configuration\\Tests\\": "tests/Configuration"
    }
  },
  "archive": {
    "exclude": [
      ".git",
      ".editorconfig",
      ".travis.yml",
      ".phpunit.xml",
      "*.sublime-*",
      "*.code-*",
      "tests"
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url" : "https://dev.pascal-iske.de/packages/"
    }
  ],
  "require": {
    "php": ">=5.6",
    "symfony/yaml": "^3.2",
    "composer/composer": "^1.6"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  }
}
